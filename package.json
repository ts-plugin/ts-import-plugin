{
  "name": "ts-import-plugin",
  "version": "1.5.5",
  "description": "babel-plugin-import TypeScript version",
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "build": "rm -rf lib && tsc -p src/tsconfig.json",
    "cover": "rm -rf ./coverage ./.nyc_output && nyc --reporter=html --reporter=lcov --exclude=node_modules --exclude=test mocha && nyc report",
    "lint": "tslint -c ./tslint.json -p ./src/tsconfig.json",
    "prepare": "npm run build",
    "start": "rm -rf dist && NODE_ENV=production webpack",
    "test": "mocha",
    "watch": "tsc -p ./src/tsconfig.json -w --diagnostics"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Brooooooklyn/ts-import-plugin.git"
  },
  "keywords": [
    "antd",
    "TypeScript",
    "plugin"
  ],
  "files": [
    "index.d.ts",
    "lib/"
  ],
  "author": "lynweklm@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/lodash": "^4.14.132",
    "@types/material-ui": "^0.21.6",
    "@types/mocha": "^5.2.6",
    "@types/node": "^12.0.2",
    "@types/react": "^16.8.18",
    "@types/react-dom": "^16.8.4",
    "antd": "^3.18.2",
    "chai": "^4.2.0",
    "coveralls": "^3.0.3",
    "css-loader": "^2.1.1",
    "husky": "^2.3.0",
    "lint-staged": "^8.1.7",
    "lodash": "^4.17.11",
    "material-ui": "^0.20.2",
    "mini-css-extract-plugin": "^0.6.0",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "prettier": "^1.17.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "rxjs": "^6.5.2",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.0.1",
    "ts-node": "^8.1.1",
    "tslint": "^5.16.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.4.5",
    "webpack": "^4.32.2",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.2"
  },
  "dependencies": {
    "tslib": "^1.9.3"
  },
  "standard": {
    "globals": [
      "describe",
      "it"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always",
    "parser": "typescript"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "tslint -c tslint.json -p tsconfig.json --fix -e \"**/test/expect/**\"",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
