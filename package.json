{
  "name": "ts-import-plugin",
  "version": "1.5.5",
  "description": "babel-plugin-import TypeScript version",
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "build": "rm -rf lib && tsc -p src/tsconfig.json",
    "cover": "rm -rf ./coverage ./.nyc_output && nyc --reporter=html --reporter=lcov --exclude=node_modules --exclude=test mocha && nyc report",
    "lint": "tslint -c ./tslint.json -p ./src/tsconfig.json",
    "prepare": "npm run build",
    "start": "rm -rf dist && NODE_ENV=production webpack",
    "test": "mocha",
    "watch": "tsc -p ./src/tsconfig.json -w --diagnostics"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Brooooooklyn/ts-import-plugin.git"
  },
  "keywords": [
    "antd",
    "TypeScript",
    "plugin"
  ],
  "files": [
    "index.d.ts",
    "lib/"
  ],
  "author": "lynweklm@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/lodash": "^4.14.118",
    "@types/material-ui": "^0.21.5",
    "@types/mocha": "^5.2.5",
    "@types/node": "^11.9.0",
    "@types/react": "^16.7.6",
    "@types/react-dom": "^16.0.9",
    "antd": "^3.16.1",
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "css-loader": "^2.0.0",
    "husky": "^1.1.4",
    "lint-staged": "^8.0.5",
    "lodash": "^4.17.11",
    "material-ui": "^0.20.2",
    "mini-css-extract-plugin": "^0.6.0",
    "mocha": "^6.0.0",
    "nyc": "^14.0.0",
    "prettier": "^1.15.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "rxjs": "^6.3.3",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.0",
    "ts-node": "^8.0.3",
    "tslint": "^5.11.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.1.6",
    "webpack": "^4.25.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "tslib": "^1.9.3"
  },
  "standard": {
    "globals": [
      "describe",
      "it"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always",
    "parser": "typescript"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "tslint -c tslint.json -p tsconfig.json --fix -e \"**/test/expect/**\"",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
